{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "browser": true,
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": 0
    }
}
